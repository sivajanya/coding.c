#include <stdio.h>

int main() {
    int arr[5];
    int i;
    printf( "Enter elements into array\n ");
    for(i=0;i<5;i++)
    {
        scanf( "%d",&arr[i]);
    } 
    printf( "the prime of array\n");
    int j;
    int flag;
    for(i=5;i>=0;i--)
    {
    flag=0;
     for(j=2;j<arr[i];j++)
     {
         if(arr[i]%j==0)
         {
             flag=1;
             break;
         }
     }
     if(flag==0)
     {
         printf( "%d\t",arr[i]);
     }
    }

    return 0;
}
