#include <stdio.h>
int main() { 
   char str[100];
   int i,count=1;
   printf("enter string\n");
   gets(str);
   printf("no of words:\n");
   for(i=0;str[i]!='\0';i++)
   {
      
       if(str[i]==' ')
       {
           count++;
       
       }
       
   }
  
printf("%d",count);
         return 0;
}
